{
  "name": "seleniumjs",
  "version": "1.7.4",
  "description": "Transforms Selenium IDE test output into javascript tests and more.",
  "main": "build/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "rimraf build && mkdirp build && cp -R src/* build/",
    "build": "tsc",
    "prebrowserify": "rimraf bundle.js && npm run build",
    "browserify": "browserify build/index.js --standalone seleniumjs -o bundle.js",
    "create-test": "node scripts/create-test/index.js",
    "lint": "tslint 'src/**/*.ts'",
    "clean": "rimraf .tmp && mkdir -p .tmp/{src,test} && cp -R src test .tmp/",
    "pretest": "npm run clean && cd test && tsc --outDir ../.tmp && cd ..",
    "test": "ava .tmp/test/unit",
    "test:generated": "npm run create-test; _mocha ./tests_generated --recursive",
    "codecov": "cat ./coverage/lcov.info | codecov",
    "bump-version": "standard-version",
    "postbump-version": "git push origin master --follow-tags"
  },
  "license": "MIT",
  "dependencies": {
    "cheerio": "^0.22.0",
    "ejs": "^2.5.6",
    "escodegen": "^1.8.0",
    "esprima": "^3.1.1",
    "estemplate": "^0.5.1",
    "estraverse": "^4.2.0",
    "front-matter": "^2.1.2",
    "js-prettify": "^1.4.0",
    "lodash.clonedeep": "^4.3.2",
    "marked": "^0.5.1"
  },
  "files": [
    "build/",
    "README.md",
    "CHANGELOG.md",
    "bundle.js"
  ],
  "ava": {
    "verbose": true,
    "files": [
      "./.tmp/test"
    ]
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.1",
    "@types/ejs": "^2.3.33",
    "@types/escodegen": "0.0.6",
    "@types/estraverse": "0.0.6",
    "@types/expect": "^1.20.1",
    "@types/lodash": "^4.14.58",
    "@types/marked": "0.0.28",
    "@types/mocha": "^2.2.32",
    "@types/node": "^7.0.9",
    "ava": "^0.25.0",
    "bluebird": "^3.4.0",
    "body-parser": "^1.18.3",
    "brfs": "^1.4.3",
    "browserify": "^14.0.0",
    "codecov": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "cz-conventional-changelog": "^1.1.6",
    "expect": "^1.20.1",
    "express": "^4.16.4",
    "fs": "0.0.2",
    "istanbul": "^0.4.3",
    "mocha": "^5.2.0",
    "multiparty": "^4.1.3",
    "node-mkdirp": "0.0.1",
    "path": "^0.12.7",
    "require-dir": "^0.3.0",
    "rimraf": "^2.6.1",
    "standard-version": "^4.4.0",
    "ts-node": "^2.1.1",
    "tslint": "^5.3.2",
    "typescript": "^2.2.2",
    "zombie": "^6.1.3"
  },
  "keywords": [
    "test",
    "selenium",
    "javascript"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  }
}
